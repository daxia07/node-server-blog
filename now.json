{
  "version": 2,
  "builds": [{ "src": "/src/index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/user",
      "dest": "/src/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "/src/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/external",
      "dest": "/src/index.js",
      "methods": ["GET"]
    }
  ]
}